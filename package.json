    "@delon/auth": "^9.2.1",
    "@delon/cache": "^9.2.1",
    "@delon/form": "^9.2.1",
    "@delon/mock": "^9.2.1",
    "@delon/theme": "^9.2.1",
    "@delon/util": "^9.2.1",
    "@delon/chart": "^9.2.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-charts": "^13.0.2",
